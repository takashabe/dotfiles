[user]
  email = takashabe.io@gmail.com
  name = takashabe
[color]
  ui = true
[core]
  quotepath = false
  excludesfile = ~/.gitignore
[merge]
  tool = vimdiff
[diff]
  tool = vimdiff
[ghq]
  root = ~/dev/src
[includeif "gitdir:/home/**"]
  path = ~/dotfiles/.gitconfig.credential.linux
[includeif "gitdir:/Users/**"]
  path = ~/dotfiles/.gitconfig.credential.darwin

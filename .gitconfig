[user]
  email = i@takashabe.dev
  name = takashabe
[color]
  ui = true
[core]
  quotepath = false
  excludesfile = ~/.gitignore
  pager = delta
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true
    light = false
    side-by-side = true
    line-numbers = true
[merge]
  tool = vimdiff
[diff]
  tool = vimdiff
[ghq]
  root = ~/dev/src
[include]
  path = ~/.gitconfig.credential
[pull]
  rebase = false

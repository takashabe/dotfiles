Xcursor.size: 12
Xft.antialias: true

! === onedark color scheme ===
! special
*.foreground:   #ABB2BF
*.background:   #282C34
*.cursorColor:  #ABB2BF
! black
*.color0:       #5C6370
*.color8:       #4B5263
! red
*.color1:       #E06C75
*.color9:       #BE5046
*.color2:       #98C379
*.color10:      #98C379
! yellow
*.color3:       #E5C07B
*.color11:      #D19A66
! blue
*.color4:       #61AFEF
*.color12:      #61AFEF
! magenta
*.color5:       #C678DD
*.color13:      #C678DD
! cyan
*.color6:       #56B6C2
*.color14:      #56B6C2
! white
*.color7:       #ABB2BF
*.color15:      #3E4452

! === urxvt ===
! fonts
urxvt.font:            xft:SF Mono Square:size=10
urxvt.letterSpace:     -1
! input method
urxvt.perl-ext-common: xim-onthespot
urxvt.inputMethod:     fcitx
urxvt.preeditType:     OnTheSpot
! key
urxvt.iso14755:        false
urxvt.iso14755_52:     false
! appearance
urxvt.pointerBlank:    true
urxvt.visualBell:      true
urxvt.scrollBar:       false
